#!/bin/bash

cli setup-operator \
    --bls-password {{ bls_password }} \
    --ecdsa-password {{ ecdsa_password }} \
    --config {{ config }} \
    --strategy-address {{ strategy_address }} \
    --strategy-deposit-amount {{ strategy_deposit_amount }}

operator \
    --bls-password {{ bls_password }} \
    --ecdsa-password {{ ecdsa_password }} \
    --config {{ config }}